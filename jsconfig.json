{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@services/*": ["./src/services/*"],
      "@views/*": ["./src/views/*"],
      "@styles/*": ["./src/styles/*"],
      "@icons/*": ["./src/assets/icons/*"],
      "@models/*": ["./src/models/*"],
      "@composables/*": ["./src/composables/*"],
    }
  },
  "exclude": ["node_modules", "dist"]
}
